{
  "private": true,
  "name": "opt-cli-test-for-package-json",
  "version": "1.2.3",
  "description": "Mocks a module's package json to retrieve a opt-cli config from.",
  "keywords": [
    "foo",
    "bar"
  ],
  "license": "MIT",
  "config": {
    "fubar": {
      "foo": "bar"
    },
    "opt": {
      "in": [
        "package-json-opted-in-foo-rule",
        "package-json-opted-in-bar-rule"
      ],
      "out": [
        "package-json-opted-out-foo-rule",
        "package-json-opted-out-bar-rule"
      ]
    }
  }
}
