{
  "extends": "../.eslintrc.json",
  "env": {
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "rules": {
    "global-require": [ 0 ],
    "no-magic-numbers": [ 0 ],
    "id-length": [ 2, { "exceptions": [ "t" ] } ]
  }
}
